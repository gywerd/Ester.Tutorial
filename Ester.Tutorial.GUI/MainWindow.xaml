                         Text="Standard"
                         FontSize="18"
                         Height="40" Width="275"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="295,150,0,0"
                         VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxCountry"
                         Text="Danmark"
                         FontSize="18"
                         Height="40" Width="450"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="120,200,0,0"
                         VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxIntAddress2"
                         Text=""
                         FontSize="18"
                         Height="40" Width="450"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="120,250,0,0"
                         VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxEMail"
                         Text="jens@pedersen.mail.dk"
                         FontSize="18"
                         Height="40" Width="450"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="120,300,0,0"
                         VerticalAlignment="Top"/>
            </Grid>
            <Grid x:Name="midRightftGrid"
                  Height="320" Width="260"
                  HorizontalAlignment="Right"
                  Margin="0,50,20,0"
                  ShowGridLines="True"
                  VerticalAlignment="Top">
                <StackPanel Height="240"
                            HorizontalAlignment="Left"
                            Margin="0,0,0,0"
                            VerticalAlignment="Top">
                    <Button x:Name="buttonUpdateSubscriber" 
                            Content="Opdater abonnent"
                            FontSize="18"
                            Height="40" Width="260"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top" 
                            Click="ButtonUpdateSubscriber_Click" />
                    <Button x:Name="buttonCreateNew" 
                            Content="Opret ny abonnent"
                            FontSize="18"
                            Height="40" Width="260"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top" 
                            Click="ButtonCreateNew_Click" />
                    <Button x:Name="buttonSearch" 
                            Content="Søgning"
                            FontSize="18"
                            Height="40" Width="260"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top" />
                    <Button x:Name="buttonShow" 
                            Content="Vis Søgeresultat"
                            FontSize="18"
                            Height="40" Width="260"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top" />
                    <Button x:Name="buttonShowGiftSubscription" 
                            Content="Vis Gaveabonnement"
                            FontSize="18"
                            Height="40" Width="260"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top" />

