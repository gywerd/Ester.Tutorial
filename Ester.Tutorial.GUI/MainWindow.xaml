                                   FontSize="18"
                                   Height="40" Width="250"
                                   HorizontalAlignment="Left"
                                   IsReadOnly="True"
                                   Margin="320,0,0,0"
                                   VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxName"
                         Text="Jens Pedersen"
                         FontSize="18"
                         Height="40" Width="480"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="90,50,0,0"
                         VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxAddress"
                         Text="Højgårdsvej 1"
                         FontSize="18"
                         Height="40" Width="480"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="90,100,0,0"
                         VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxPlace"
                         Text="Neder Skringstrup"
                         FontSize="18"
                         Height="40" Width="480"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="90,150,0,0"
                         VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxZip"
                         Text="8832"
                         FontSize="18"
                         Height="40" Width="50"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="90,200,0,0"
                         VerticalAlignment="Top"/>
                <Label x:Name="labelTown"
                       Content="By:"
