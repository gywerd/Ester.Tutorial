                <Label x:Name="labelBalance"
                       Content="Balance:"
                       FontSize="18"
                       Height="40" Width="75"
                       HorizontalAlignment="Left"
                       Margin="0,260,0,0"
                       VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxBalance"
                         Text="598,00"
                         FontSize="18"
                         Height="40" Width="170"
                         HorizontalAlignment="Left"
                         HorizontalContentAlignment="Right"
                         IsReadOnly="True"
                         Margin="90,250,0,0"
                         VerticalAlignment="Top"/>
            </Grid>
        </Grid>
        <Grid x:Name="bottomGrid"
              Width="1440"
              Height="340"
              VerticalAlignment="Bottom"
              Margin="0,0,0,0"
              ShowGridLines="True">
            <TabControl Margin="5,0,10,5">
                <TabItem Header="Abonnement">
                    <Grid Background="#FFE5E5E5">
                        <Button x:Name="buttonNewSubscription" 
                            Content="Nyt Abonnement"
                            Margin="20,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Width="150"
                            Height=" 30" 
                            FontSize="18" />
                        <Label x:Name="labelInvoiceNumber" 
                               Content="Fakturanr:" 
                               Width="150" 
                               Height="30" 
                               HorizontalAlignment="Left"
                               HorizontalContentAlignment="Right"
                               VerticalAlignment="Top" 
                               FontSize="18" Margin="180,0,0,0"/>
                        <TextBox x:Name="textBoxInvoiceNumber" 
                                 Text="115413" 
                                 Width="150" 
                                 Height="30" 
                                 HorizontalAlignment="Left" 
                                 VerticalAlignment="Top" 
                                 FontSize="18" 
                                 Margin="340,0,0,0" 
                                 IsReadOnly="True" 
                                 Background="AntiqueWhite"/>
                        <Label x:Name="labelPostponeTo" 
                               Content="Henstand til:" 
                               Width="150" 
                               Height="30" 
                               HorizontalAlignment="Left"
                               HorizontalContentAlignment="Right"
                               VerticalAlignment="Top" 
                               FontSize="18"
                               Margin="500,0,0,0"/>
                        <TextBox x:Name="textBoxPostponeTo" 
                                 Text="" 
                                 Width="150" 
                                 Height="30" 
                                 HorizontalAlignment="Left" 
                                 VerticalAlignment="Top" 
                                 FontSize="18" 
                                 Margin="660,0,0,0" 
                                 IsReadOnly="True" 
                                 Background="AntiqueWhite"/>
                        <Label x:Name="labelGift" 
                               Content="Gave:" 
                               Width="150" 
                               Height="30" 
                               HorizontalAlignment="Left"
                               HorizontalContentAlignment="Right"
                               VerticalAlignment="Top" 
                               FontSize="18" 
                               Margin="820,0,0,0"/>
                        <TextBox x:Name="textBoxGift" 
                                 Text="" 
                                 Width="150" 
                                 Height="30" 
                                 HorizontalAlignment="Left" 
                                 VerticalAlignment="Top" 
                                 FontSize="18" 
                                 Margin="980,0,0,0" 
                                 IsReadOnly="True" 
                                 Background="AntiqueWhite"/>
                        <Button x:Name="buttonDelete" 
                            Content="Slet"
                            Margin="0,0,20,0"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Top"
                            Width="50"
                            Height=" 30" 
                            FontSize="18" />
                        <DataGrid x:Name="dataGridSubscriptions"
                                  Width="1440" 
                                  Margin="0,40,0,40"
                                  AutoGenerateColumns="True"
                                  ItemsSource="{Binding subscriber}"
                                  DisplayMemberPath="Subscriptions"/>
                    </Grid>
                </TabItem>
                <TabItem Header="Gaveabonnement">
                    <Grid Background="#FFE5E5E5"/>
                </TabItem>
                <TabItem Header="Kassekladde">
                    <Grid Background="#FFE5E5E5">
                        <DataGrid x:Name="dataGridRegistrationDraft"
                                  Width="1300" 
                                  Margin="0,20,140,40"
                                  DisplayMemberPath="RegistrationDrafts"/>
                        <Button x:Name="buttonRegistrationDraft" 
                                Content="Kassekladde"
                                Margin="0,20,20,0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Top"
                                Width="100"
                                Height="100" 
                                FontSize="16" Click="ButtonRegistrationDraft_Click" />

                    </Grid>
                </TabItem>
                <TabItem Header="PBS Betaling">
                    <Grid Background="#FFE5E5E5">
                        <Grid Height="187.2" Width="400"
                              HorizontalAlignment="Left"
                              Margin="10,10"
                              VerticalAlignment="Center">
                            <CheckBox x:Name="checkBoxPBSStartDato" 
                                   Content="PBS:" 
                                   Width="190" 
                                   Height="30" 
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Top" 
                                   FontSize="18" 
                                   Margin="0,0,0,0"/>
                            <ComboBox x:Name="comboBoxPBSStartDato" 
                                     Text="" 
                                     Width="190" 
                                     Height="30" 
                                     HorizontalAlignment="Left" 
                                     VerticalAlignment="Top" 
                                     FontSize="18" 
                                     Margin="200,0,0,0" 
                                     IsReadOnly="True" 
                                     Background="AntiqueWhite"/>
                            <Label x:Name="labelPBSStartDato" 
                                   Content="PBS Start Dato:" 
                                   Width="190" 
                                   Height="30" 
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Top" 
                                   FontSize="18" 
                                   Margin="0,40,0,0"/>
                            <TextBox x:Name="textBoxPBSStartDato" 
                                     Text="" 
                                     Width="190" 
                                     Height="30" 
                                     HorizontalAlignment="Left" 
                                     VerticalAlignment="Top" 
                                     FontSize="18" 
                                     Margin="200,40,0,0" 
                                     IsReadOnly="True" 
                                     Background="AntiqueWhite"/>
                            <Label x:Name="labelPBSStopDato" 
                                   Content="PBS Stop Dato:" 
                                   Width="190" 
                                   Height="30" 
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Top" 
                                   FontSize="18" 
                                   Margin="0,90,0,0"/>
