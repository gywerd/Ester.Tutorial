                          Width="165"
                          Height="40"
                          HorizontalAlignment="Left"
                          Margin="275,50,0,0"
                          VerticalAlignment="Top"/>
                <CheckBox x:Name="checkBoxNewsletter"
                          Content="Nyhedsbrev"
                          FontSize="18"
                          Height="40" Width="125"
                          HorizontalAlignment="Left"
                          Margin="450,50,0,0"
                          VerticalAlignment="Top"/>
