            <Grid x:Name="midCenterGrid"
                  Width="580"
                  Height="370"
                  VerticalAlignment="Top"
                  HorizontalAlignment="Left"
                  Margin="580,0,0,0"
                  ShowGridLines="True">
                <Label x:Name="labelId"
                       Content="ID:"
                       FontSize="18"
                       Height="40" Width="30"
                       HorizontalAlignment="Left"
                       Margin="0,0,0,0"
                       VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxId"
                         Text="TH"
                         Background="AntiqueWhite"
                         FontSize="18"
                         Width="50"
                         Height="40"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="40,0,0,0"
                         VerticalAlignment="Top"/>
                <Label x:Name="labelDateChanged"
                       Content="Rettet dato:"
                       FontSize="18"
                       Height="40" Width="105"
                       HorizontalAlignment="Left"
                       Margin="100,0,0,0"
                       VerticalAlignment="Top"/>
                <DatePickerTextBox x:Name="textBoxDateChanged"
                                   Text="03-02-2017"
                                   Background="AntiqueWhite"
                                   FontSize="18"
                                   Height="40" Width="350"
                                   HorizontalAlignment="Left"
                                   IsReadOnly="True"
                                   Margin="215,0,0,0"
                                   VerticalAlignment="Top"/>
                <CheckBox x:Name="checkBoxDead"
                          Content="Død"
                          FontSize="18"
                          Height="40" Width="60"
                          HorizontalAlignment="Left"
                          Margin="0,50,0,0"
