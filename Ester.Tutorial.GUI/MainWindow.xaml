                    Content="Lister" 
                    FontSize="18" 
                    Grid.Column="1"
                    Height="40" Width="100"
                    HorizontalAlignment="Left"
                    Margin="560,10,0,0"
                    VerticalAlignment="Top"/>
            <Button x:Name="buttonOkonomi" 
                    Content="Økonomi" 
                    FontSize="18" 
