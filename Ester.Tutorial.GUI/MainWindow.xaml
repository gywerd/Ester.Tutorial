                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="195,200,0,0"
                         VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxIntAddress"
                         Text=""
                         FontSize="18"
                         Height="40" Width="480"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="90,250,0,0"
                         VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxPhonePrefix"
                         Text="+45"
                         FontSize="18"
                         Height="40" Width="40"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="90,300,0,0"
                         VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxPhone"
                         Text="86697777"
                         FontSize="18"
                         Height="40" Width="135"
                         HorizontalAlignment="Left"
                         IsReadOnly="True"
                         Margin="140,300,0,0"
                         VerticalAlignment="Top"/>
