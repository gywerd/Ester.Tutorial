                                     Text="" 
                                     Width="190" 
                                     Height="30" 
                                     HorizontalAlignment="Left" 
                                     VerticalAlignment="Top" 
                                     FontSize="18" 
                                     Margin="200,90,0,0" 
                                     IsReadOnly="True" 
                                     Background="AntiqueWhite"/>

                        </Grid>
                    </Grid>
                </TabItem>
                <TabItem Header="Koder">
                    <Grid Background="#FFE5E5E5"/>
                </TabItem>
                <TabItem Header="Bemærkninger">
                    <Grid Background="#FFE5E5E5">
                        <Label x:Name="labelComment"
                               Content="Bemærkning:"
                               FontSize="18"
                               Height="40" Width="120"
                               HorizontalAlignment="Left"
                               Margin="10,0,0,0"
                               VerticalAlignment="Top"/>
                        <TextBox x:Name="textBoxComment"
                                 Text=""
                                 FontSize="18"
                                 Height="40" Width="1020"
                                 HorizontalAlignment="Left"
                                 Margin="135,0,0,0"
                                 VerticalAlignment="Top"/>
                        <Button x:Name="buttonTypeComment" 
                                Content="Tilføj bemærkning"
                                FontSize="18"
                                Height=" 40" Width="260"
                                HorizontalAlignment="Right"
                                Margin="0,0,10,0"
                                VerticalAlignment="Top" Click="ButtonTypeComment_Click" />
                        <DataGrid x:Name="dataGridComments"
                                  Width="1440" 
                                  Margin="0,40,0,40"
                                  AutoGenerateColumns="True"
                                  DisplayMemberPath="Subscriptions"/>

