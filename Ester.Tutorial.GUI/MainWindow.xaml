                         Text="1"
                         FontSize="18"
                         Height="40" Width="80"
                         HorizontalAlignment="Left"
                         HorizontalContentAlignment="Right"
                         IsReadOnly="True"
                         Margin="120,100,0,0"
                         VerticalAlignment="Top"/>
                <TextBox x:Name="textBoxAmount"
                         Text="1"
                         FontSize="18"
                         Height="40" Width="80"
                         HorizontalAlignment="Left"
                         HorizontalContentAlignment="Right"
                         IsReadOnly="True"
                         Margin="120,150,0,0"
                         VerticalAlignment="Top"/>
                <Label x:Name="labelGroup"
                       Content="Gruppe:"
                       FontSize="18"
                       Height="40" Width="75"
                       HorizontalAlignment="Left"
                       Margin="210,150,0,0"
                       VerticalAlignment="Top"/>
